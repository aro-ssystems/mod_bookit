<tr class="table-default bookit-checklistitem"
    data-bookit-checklistitem-id="{{id}}"
    data-bookit-checklistitem-name="{{name}}"
    data-bookit-checklistitem-order="{{order}}"
    data-bookit-checklistitem-category="{{categoryid}}">
                <td>{{name}}</td>
                <td>{{roomname}}</td>
                <td>RESPONSIBILITY</td>
                <td>{{#pix}} i/edit, core {{/pix}}</td>
                <td>{{#pix}} t/sort, core {{/pix}}</td>
</tr>
